<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="freight.form">
      <field name="name">test form</field>
      <field name="model">test3.test3</field>
      <field name="arch" type="xml">
        <form>
          <field name="name"/>
          <field name="mohamedomar"/>
          <!-- <field name="test_tmpl_id"/> -->
          <field name="cost" eval="0.0" />

        </form>
      </field>
    </record>
    
    <record model="ir.ui.view" id="freight.list">
      <field name="name">test list</field>
      <field name="model">test3.test3</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="mohamedomar"/>
          <!-- <field name="test_tmpl_id"/> -->
          
          <field name="cost" eval="0.0" />
          
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="freight.test3_action_window">
      <field name="name">freight window</field>
      <field name="res_model">test3.test3</field>
      <field name="view_mode">tree,form</field>
    </record>
 <menuitem name="freight" id="freight.menu_root" action="freight.test3_action_window"/>
